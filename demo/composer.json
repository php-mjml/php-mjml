{
    "name": "php-mjml/demo",
    "description": "Demo for php-mjml",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "../"
        }
    ],
    "require": {
        "php-mjml/php-mjml": "@dev",
        "symfony/css-selector": "^8.0",
        "symfony/dom-crawler": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/html-sanitizer": "^8.0",
        "symfony/options-resolver": "^8.0",
        "symfony/runtime": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "David Gorges",
            "email": "gorges@dekkode.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
